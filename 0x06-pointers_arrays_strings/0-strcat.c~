#include "main.h"

/**
 *_strcat - concenates string
 *@dest: destination
 *@src: source
 *Return: 0
 */

char *_strcat(char *dest, char *src)
{
        char i, j;

        for(i = 0; src[i] != '\0'; ++i);

        for(j = 0; dest[j] != '\0'; ++j, ++i)
        {
                src[i] = dest[j];
	}
	dest[i] = '\0';
	_putchar(dest);

	return 0;
}
